{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"name": "status"
								}
							},
							{
								"source": {
									"path": "$['totalResults']"
								},
								"sink": {
									"name": "totalResults"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['source']['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['source']['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['author']"
								},
								"sink": {
									"name": "author"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['title']"
								},
								"sink": {
									"name": "title"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['description']"
								},
								"sink": {
									"name": "description"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['url']"
								},
								"sink": {
									"name": "url"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['urlToImage']"
								},
								"sink": {
									"name": "urlToImage"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['publishedAt']"
								},
								"sink": {
									"name": "publishedAt"
								}
							},
							{
								"source": {
									"path": "$['articles'][0]['content']"
								},
								"sink": {
									"name": "content"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "RestResource1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"Rupert": {
				"type": "string"
			}
		},
		"annotations": []
	}
}